{
  "manifest_version": 3,
  "name": "Bílasölur – extra specs on list",
  "version": "1.3",
  "description": "Adds horsepower, city fuel consumption, tire set and last updated date to bilasolur.is listings.",
  "host_permissions": ["https://bilasolur.is/*"],
  "content_scripts": [
    {
      "matches": [
        "https://bilasolur.is/SearchResults.aspx*",
        "https://bilasolur.is/Section.aspx*",
        "https://bilasolur.is/default.aspx/Section.aspx*",
        "https://bilasolur.is/defaultbilasolur.aspx/Section.aspx*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ]
}